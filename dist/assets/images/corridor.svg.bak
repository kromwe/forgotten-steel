<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="corridorGradient" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#1a1a1a;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#2a2a2a;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1a1a1a;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="stoneWall" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#4a4a4a;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2a2a2a;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="600" fill="url(#corridorGradient)"/>
  
  <!-- Stone floor pattern -->
  <g stroke="#333" stroke-width="1" fill="none">
    <line x1="0" y1="500" x2="800" y2="500"/>
    <line x1="0" y1="520" x2="800" y2="520"/>
    <line x1="0" y1="540" x2="800" y2="540"/>
  </g>
  
  <!-- Walls -->
  <rect x="0" y="0" width="800" height="100" fill="url(#stoneWall)"/>
  <rect x="0" y="500" width="800" height="100" fill="url(#stoneWall)"/>
  
  <!-- Pillars -->
  <rect x="150" y="100" width="40" height="400" fill="#3a3a3a" stroke="#5a5a5a" stroke-width="2"/>
  <rect x="350" y="100" width="40" height="400" fill="#3a3a3a" stroke="#5a5a5a" stroke-width="2"/>
  <rect x="550" y="100" width="40" height="400" fill="#3a3a3a" stroke="#5a5a5a" stroke-width="2"/>
  
  <!-- Torch brackets with flickering light -->
  <rect x="140" y="150" width="60" height="20" fill="#4a4a4a"/>
  <circle cx="170" cy="140" r="12" fill="#ff6600" opacity="0.8">
    <animate attributeName="opacity" values="0.6;1.0;0.6" dur="1.5s" repeatCount="indefinite"/>
  </circle>
  
  <rect x="540" y="150" width="60" height="20" fill="#4a4a4a"/>
  <circle cx="570" cy="140" r="12" fill="#ff6600" opacity="0.8">
    <animate attributeName="opacity" values="1.0;0.6;1.0" dur="1.8s" repeatCount="indefinite"/>
  </circle>
  
  <!-- Doorways -->
  <rect x="375" y="500" width="50" height="100" fill="#0a0a0a"/> <!-- South exit -->
  <rect x="0" y="275" width="100" height="50" fill="#0a0a0a"/> <!-- West exit -->
  <rect x="700" y="275" width="100" height="50" fill="#0a0a0a"/> <!-- East exit -->
  
  <!-- Ancient symbols on walls -->
  <text x="400" y="80" text-anchor="middle" fill="#666" font-family="serif" font-size="24">⚔ ⚡ ⚔</text>
  
  <!-- Title text -->
  <text x="400" y="580" text-anchor="middle" fill="#888" font-family="serif" font-size="16">Stone Corridor</text>
</svg>